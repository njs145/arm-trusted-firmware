{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug BL31",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/qemu/debug/bl31/bl31.elf",  
        "miDebuggerPath": "/opt/arm-gnu-toolchain-14.2.rel1-x86_64-aarch64-none-elf/bin/aarch64-none-elf-gdb",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${workspaceFolder}",
        "MIMode": "gdb",
        "targetArchitecture": "arm64",
        "setupCommands": [
          {
            "text": "target remote :1234",
            "ignoreFailures": true
          },
          {
            "text": "add-symbol-file /home/song/study/arm-trusted-firmware/build/qemu/debug/bl31/bl31.elf 0xe090000",
            "ignoreFailures": true
          },
          {
            "text": "b bl31_entrypoint",
            "ignoreFailures": true
          },
          {
            "text": "continue",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  